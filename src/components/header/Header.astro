---
import { cn } from "@lib/utils";
import PageLinks from "./PageLinks.astro";

const sections = [
  { name: "Home", href: "/" },
  { name: "About", href: "#about" },
  { name: "Contact", href: "#contact" },
  { name: "Blog", href: "#blog" }
];
interface Props {
  class?: string;
}
const { class: className } = Astro.props;
---

  <header class={cn("h-header", className)}>
    <PageLinks pages={sections} />
  </header>
